{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "tymon/jwt-auth": "0.5.*",
        "dingo/api": "1.0.x@dev",
        "barryvdh/laravel-cors": "^0.7.1",
        "barryvdh/laravel-ide-helper": "^2.1",
        "doctrine/dbal": "^2.5",
        "nesbot/carbon": "^1.21",
        "intervention/image": "^2.3",
        "spatie/laravel-glide": "^2.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php -r \"mkdir(__DIR__.'/storage/framework');\"",
            "php -r \"mkdir(__DIR__.'/bootstrap/cache');\"",
            "php -r \"mkdir(__DIR__.'/storage/framework/sessions');\"",
            "php -r \"mkdir(__DIR__.'/storage/framework/views');\"",
            "php -r \"mkdir(__DIR__.'/storage/framework/cache');\"",
            "php artisan cache:clear",
            "php artisan config:clear",
            "php artisan view:clear",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
